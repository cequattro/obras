package pe.gob.sunafil.denunciavirtual.bean;

import java.io.Serializable;

public class Persona extends Auditoria implements Serializable{

	private static final long serialVersionUID = 5480278979942086740L;

	private String v_numdoc;
	private String v_codtdocide;
	private String v_appaterno;
	private String v_apmaterno;
	private String v_nombres;
	private String v_coddepdom;	
	private String v_codprovdom;
	private String v_coddisdom;
	private String v_dirdom;
	private String v_codverific;
	private String v_flgreniec;
	
	public String getV_numdoc() {
		return v_numdoc;
	}
	public void setV_numdoc(String v_numdoc) {
		this.v_numdoc = v_numdoc;
	}
	public String getV_codtdocide() {
		return v_codtdocide;
	}
	public void setV_codtdocide(String v_codtdocide) {
		this.v_codtdocide = v_codtdocide;
	}
	public String getV_appaterno() {
		return v_appaterno;
	}
	public void setV_appaterno(String v_appaterno) {
		this.v_appaterno = v_appaterno;
	}
	public String getV_apmaterno() {
		return v_apmaterno;
	}
	public void setV_apmaterno(String v_apmaterno) {
		this.v_apmaterno = v_apmaterno;
	}
	public String getV_nombres() {
		return v_nombres;
	}
	public void setV_nombres(String v_nombres) {
		this.v_nombres = v_nombres;
	}
	public String getV_coddepdom() {
		return v_coddepdom;
	}
	public void setV_coddepdom(String v_coddepdom) {
		this.v_coddepdom = v_coddepdom;
	}
	public String getV_codprovdom() {
		return v_codprovdom;
	}
	public void setV_codprovdom(String v_codprovdom) {
		this.v_codprovdom = v_codprovdom;
	}
	public String getV_coddisdom() {
		return v_coddisdom;
	}
	public void setV_coddisdom(String v_coddisdom) {
		this.v_coddisdom = v_coddisdom;
	}
	public String getV_dirdom() {
		return v_dirdom;
	}
	public void setV_dirdom(String v_dirdom) {
		this.v_dirdom = v_dirdom;
	}
	public String getV_codverific() {
		return v_codverific;
	}
	public void setV_codverific(String v_codverific) {
		this.v_codverific = v_codverific;
	}
	public String getV_flgreniec() {
		return v_flgreniec;
	}
	public void setV_flgreniec(String v_flgreniec) {
		this.v_flgreniec = v_flgreniec;
	}
	@Override
	public String toString() {
		return "Persona [v_numdoc=" + v_numdoc + ", v_codtdocide="
				+ v_codtdocide + ", v_appaterno=" + v_appaterno
				+ ", v_apmaterno=" + v_apmaterno + ", v_nombres=" + v_nombres
				+ ", v_coddepdom=" + v_coddepdom + ", v_codprovdom="
				+ v_codprovdom + ", v_coddisdom=" + v_coddisdom + ", v_dirdom="
				+ v_dirdom + ", v_codverific=" + v_codverific
				+ ", v_flgreniec=" + v_flgreniec + "]";
	}
	
}
