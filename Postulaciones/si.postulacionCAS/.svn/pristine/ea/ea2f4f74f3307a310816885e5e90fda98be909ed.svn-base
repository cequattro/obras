package pe.gob.sunafil.postulacioncas.bean;

import java.io.Serializable;

public class Intendencias implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -2988682857961395552L;
	
	private String n_numdep;
	private String v_desdep;
	private String v_flgact;
	
	public String getN_numdep() {
		return n_numdep;
	}
	public void setN_numdep(String n_numdep) {
		this.n_numdep = n_numdep;
	}
	public String getV_desdep() {
		return v_desdep;
	}
	public void setV_desdep(String v_desdep) {
		this.v_desdep = v_desdep;
	}
	public String getV_flgact() {
		return v_flgact;
	}
	public void setV_flgact(String v_flgact) {
		this.v_flgact = v_flgact;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	
	@Override
	public String toString() {
		return "Intendencias [n_numdep=" + n_numdep + ", v_desdep=" + v_desdep
				+ ", v_flgact=" + v_flgact + "]";
	}
	
	
}
