package pe.gob.sunafil.postulacioncas.bean;

import java.io.Serializable;
import java.util.Date;

public class ParamConsultaSIIT implements Serializable{
	
	
	private static final long serialVersionUID = 1L;
	
	private String v_codIntendencia;
	private String nroDocumento;
	private String apePater;
	private String apeMater;
	private String nombres;
	private String nroRegMP;
	private String anioRegMP;
	
	public ParamConsultaSIIT(){
		System.out.println("entro a constructor de ParamConsultaSIIT");
		apePater="";
		apeMater="";
		nombres="";
		v_codIntendencia="";
		nroDocumento="";
		nroRegMP="";
		anioRegMP="";
	}
	
	

	@Override
	public String toString() {
		return "ParamConsultaSIIT [v_codIntendencia=" + v_codIntendencia
				+ ", nroDocumento=" + nroDocumento + ", apePater=" + apePater
				+ ", apeMater=" + apeMater + ", nombres=" + nombres
				+ ", nroRegMP=" + nroRegMP + ", anioRegMP=" + anioRegMP + "]";
	}



	

	public String getV_codIntendencia() {
		return v_codIntendencia;
	}

	public void setV_codIntendencia(String v_codIntendencia) {
		this.v_codIntendencia = v_codIntendencia;
	}

	public String getNroDocumento() {
		return nroDocumento;
	}

	public void setNroDocumento(String nroDocumento) {
		this.nroDocumento = nroDocumento;
	}

	public String getApePater() {
		apePater=apePater.toUpperCase();
		return apePater;
	}

	public void setApePater(String apePater) {
		this.apePater = apePater;
	}

	public String getApeMater() {
		apeMater=apeMater.toUpperCase();
		return apeMater;
	}

	public void setApeMater(String apeMater) {
		this.apeMater = apeMater;
	}

	public String getNombres() {
		nombres=nombres.toUpperCase();
		return nombres;
	}

	public void setNombres(String nombres) {
		this.nombres = nombres;
	}

	public String getNroRegMP() {
		return nroRegMP;
	}

	public void setNroRegMP(String nroRegMP) {
		this.nroRegMP = nroRegMP;
	}

	public String getAnioRegMP() {
		return anioRegMP;
	}

	public void setAnioRegMP(String anioRegMP) {
		this.anioRegMP = anioRegMP;
	}

	
	

}
