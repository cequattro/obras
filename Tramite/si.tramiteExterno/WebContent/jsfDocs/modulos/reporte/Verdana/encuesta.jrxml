	<queryString>
		<![CDATA[SELECT t.v_periodo,
       t.v_numdocide,
       t.v_desrazons,
       t.v_coddep,
       (SELECT d.v_desdep FROM simintra1.sitb_departamento d WHERE t.v_coddep=d.v_coddep ) v_desdep,
       t.v_codpro,
       (SELECT d.v_despro FROM simintra1.sitb_provincia d WHERE t.v_coddep=d.v_coddep AND t.v_codpro=d.v_codpro ) v_despro,
        t.v_coddis,
       (SELECT d.v_desdis FROM simintra1.sitb_distrito d WHERE t.v_coddep=d.v_coddep AND t.v_codpro=d.v_codpro AND t.v_coddis=d.v_coddis  ) v_desdis,
       t.v_diremp,
       t.v_telemp,
       t.v_codsec,
       (SELECT s.v_dessec FROM simintra1.sitb_seccion s WHERE s.v_codsec=t.v_codsec ) v_dessec,
       t.v_codciiu,
       (SELECT c.v_desciiu FROM simintra1.sitb_ciiu c  WHERE c.v_codsec=t.v_codsec AND c.v_codciiu=t.v_codciiu ) v_desciiu,
       t.n_tietradis,
       t.n_numtrabpe,

       dm.v_apenom,
       UPPER(dm.v_cargo) as v_cargo,
       dm.v_email,
       dm.v_codtelfij||'-'||dm.v_telfijo as v_telfijo,
       dm.v_telmovil,
       dm.v_flgcontdis,
       FNC_CREPORTE(1,t.v_periodo,t.v_numdocide) v_des3233,
	oxd.n_corocdem as n_corocdem,
       (SELECT d.v_desdep FROM simintra1.sitb_departamento d WHERE oxd.v_coddep=d.v_coddep ) v_desdepo,
       (SELECT d.v_despro FROM simintra1.sitb_provincia d WHERE oxd.v_coddep=d.v_coddep AND oxd.v_codpro=d.v_codpro ) v_desproo,
       oxd.v_codocupac,
       (SELECT d.v_desocupac FROM simintra1.sitb_ocupacion d WHERE oxd.v_codocupac=d.v_codocupac ) v_desocupac,
       oxd.n_numtraba,
       FNC_DREPORTE(2,oxd.n_corocdem) v_dese,
       (SELECT ne.v_desnivesu FROM entbc_nivedu ne WHERE oxd.n_cornivedu=ne.n_cornivedu) as v_desnivesu,
       FNC_DREPORTE(3,oxd.n_corocdem) v_desg,
       oxd.v_flgcap,
       FNC_DREPORTE(4,oxd.n_corocdem) v_desi,
       oxd.v_flgexplab,
       oxd.n_monrembru,
       oxd.n_corhorari,
       (SELECT ed.v_deseda FROM enmvd_ocupxedad oxe, entbc_edad ed WHERE oxe.n_corocdem=oxd.n_corocdem AND oxe.n_coreda=ed.n_coreda) as v_deseda,
       (SELECT oxe.n_itrime FROM enmvd_ocupxedad oxe WHERE oxe.n_corocdem=oxd.n_corocdem) as n_itrime,
       (SELECT oxe.n_iitrime FROM enmvd_ocupxedad oxe WHERE oxe.n_corocdem=oxd.n_corocdem) as n_iitrime,
       (SELECT oxe.n_iiitrime FROM enmvd_ocupxedad oxe WHERE oxe.n_corocdem=oxd.n_corocdem) as n_iiitrime,
       (SELECT oxe.n_ivtrime FROM enmvd_ocupxedad oxe WHERE oxe.n_corocdem=oxd.n_corocdem) as n_ivtrime,
       oxd.n_numtracon,
       oxd.n_numtraloc,
       oxd.n_numtraotr,


       dm.v_flgpol,
       dm.v_flgorient,
       FNC_CREPORTE(5,t.v_periodo,t.v_numdocide) v_des4244,
       FNC_CREPORTE(6,t.v_periodo,t.v_numdocide) v_des51,
       dm.v_flgdific,
       FNC_CREPORTE(7,t.v_periodo,t.v_numdocide) v_des62,
       decode(dm.v_valoraci,1,'Muy útil',2,'Útil',3,'Poco útil',4,'Nada útil') as v_valoraci,
       t.n_numtrabse

FROM  enmvx_muestraemp t
INNER JOIN enmvc_demandapd dm ON t.v_periodo=dm.v_periodo AND t.v_numdocide=dm.v_numdocide
LEFT JOIN  enmvd_ocupaxdeman oxd ON t.v_periodo=oxd.v_periodo AND t.v_numdocide=oxd.v_numdocide
WHERE t.v_periodo='112013' AND t.v_numdocide='20514038270']]>
	</queryString>
